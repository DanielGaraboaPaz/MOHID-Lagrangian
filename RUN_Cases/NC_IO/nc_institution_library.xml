<?xml version="1.0" encoding="UTF-8" ?>
<nc_definitions>
	<!-- the variables names in MOHID Lagrangian -->
	<eastward_sea_water_velocity value="u" />
	<northward_sea_water_velocity value="v" />
	<upward_sea_water_velocity value="w" />
	<sea_water_temperature value="temp" />
	<sea_water_salinity value="sal" />
	<sea_water_density value="density" />
	<latitude value="lat" />
	<longitude value="longitude" />
	<depth value="depth" />
	<time value="time" />
</nc_definitions>
<nc_library>
	<MOHID>
		<!-- the the variables names in files written by MOHID -->
		<eastward_sea_water_velocity value="u" />
		<northward_sea_water_velocity value="v" />
		<upward_sea_water_velocity value="w" />
		<sea_water_temperature value="temp" />
		<sea_water_salinity value="salt" />
		<!-- Other definitions on the files written by MOHID -->
		<some_random_scale value="0.57" />
	</MOHID>
	<CMEMS>
		<!-- the the variables names in files written by CMEMS -->
		<eastward_sea_water_velocity value="uo" />
		<northward_sea_water_velocity value="vo" />
		<upward_sea_water_velocity value="w" />
		<sea_water_temperature value="temp" />
		<sea_water_salinity value="salt" />
		<!-- Other definitions on the files written by CMEMS -->
		<some_random_scale value="0.57" />
	</CMEMS>
</nc_library>